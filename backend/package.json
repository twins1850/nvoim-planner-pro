{
  "name": "english-conversation-backend",
  "version": "1.0.0",
  "description": "앤보임 영어회화 관리 시스템 백엔드",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.test.ts'",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts'",
    "test:security": "jest --testMatch='**/tests/security/**/*.test.ts'",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:v3": "jest --testMatch='**/tests/unit/**/*-v3.test.ts' --testMatch='**/tests/integration/aws-sdk-v3-migration.test.ts'",
    "test:aws": "jest --testMatch='**/tests/**/*aws*.test.ts' --testMatch='**/tests/**/*backup*.test.ts' --testMatch='**/tests/**/*file*.test.ts'",
    "performance": "artillery run performance-tests/api-load-test.yml",
    "performance:dev": "artillery run performance-tests/api-load-test.yml -e development",
    "performance:staging": "artillery run performance-tests/api-load-test.yml -e staging",
    "performance:monitoring": "artillery run performance-tests/performance-monitoring-test.yml",
    "performance:monitoring:dev": "artillery run performance-tests/performance-monitoring-test.yml -e development",
    "performance:report": "node -e \"const { performanceMonitoringService } = require('./dist/services/performanceMonitoringService'); performanceMonitoringService.getDashboardData().then(data => console.log(JSON.stringify(data, null, 2)))\"",
    "security:assessment": "node src/scripts/securityAssessment.js",
    "security:audit": "npm audit --json > security-audit.json",
    "backup:create": "node -e \"require('./dist/services/backupService').BackupService.initialize(); require('./dist/services/backupService').BackupService.createDatabaseBackup().then(path => console.log('Backup created:', path))\"",
    "backup:list": "node -e \"require('./dist/services/backupService').BackupService.initialize(); require('./dist/services/backupService').BackupService.listBackups().then(backups => console.log('Available backups:', backups))\"",
    "backup:create:v3": "node -e \"require('./dist/services/backupService-v3').BackupServiceV3.initialize(); require('./dist/services/backupService-v3').BackupServiceV3.createDatabaseBackup().then(path => console.log('Backup created (v3):', path))\"",
    "backup:list:v3": "node -e \"require('./dist/services/backupService-v3').BackupServiceV3.initialize(); require('./dist/services/backupService-v3').BackupServiceV3.listBackups().then(backups => console.log('Available backups (v3):', backups))\"",
    "backup:cleanup:v3": "node -e \"require('./dist/services/backupService-v3').BackupServiceV3.initialize(); require('./dist/services/backupService-v3').BackupServiceV3.cleanupOldBackups(30).then(count => console.log('Deleted old backups (v3):', count))\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@sentry/node": "^7.91.0",
    "@sentry/profiling-node": "^1.3.2",
    "@types/socket.io": "^3.0.1",
    "aws-sdk": "^2.1506.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "ffmpeg-static": "^5.2.0",
    "firebase-admin": "^13.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.32.0",
    "mongodb-memory-server": "^10.1.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "redis": "^4.6.10",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.9",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "artillery": "^2.0.24",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
